#!/usr/bin/env zsh
# ----------------------- Alias program -----------------------------

# tmux
alias tmux='TERM=xterm-256color tmux'

# python
alias python=python3
alias pip=pip3

# Flutter
alias fl="flutter"
alias flattach="flutter attach"
alias flb="flutter build"
alias flchnl="flutter channel"
alias flc="flutter clean"
alias fldvcs="flutter devices"
